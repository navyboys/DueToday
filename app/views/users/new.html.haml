= render 'shared/content_title', title: 'Sign Up'
.well
  = bootstrap_form_for(@user, layout: :horizontal, label_col: "col-md-4", control_col: "col-md-4") do |f|
    = f.email_field :email
    = f.password_field :password
    = f.text_field :user_name
    = f.form_group do
      = f.primary "Sign Up"